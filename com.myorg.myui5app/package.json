{
    "name": "com.myorg.myui5app",
    "version": "0.0.1",
    "private": true,
    "description": "com.myorg.myui5app description",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "dependencies": {},
    "devDependencies": {
        "@ui5/cli": "^3.0.0",
        "@sap/ux-ui5-tooling": "1",
        "mbt": "^1",
        "npm-run-all": "^4",
        "rimraf": "latest",
        "bestzip": "latest"
    },
    "scripts": {
        "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#commyorgmyui5app-tile\"",
        "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#commyorgmyui5app-tile\"",
        "build": "npm-run-all clean build:ui5 zip",
        "deploy": "cf deploy mta_archives/com.myorg.myui5app.mtar --retries 0",
        "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
        "clean": "rimraf com.myorg.myui5app-content.zip",
        "build:ui5": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
        "zip": "cd dist && bestzip ../com.myorg.myui5app-content.zip *",
        "build:mta": "mbt build --mtar com.myorg.myui5app.mtar"
    }
}