{
    "name": "<%= title %>",
    "scripts": {
        
        "build": "mbt build --mtar <%= title %>.mtar",
        "build:workspaces": "npm run build --workspaces --if-present",
        "deploy": "cf deploy mta_archives/<%= title %>.mtar --retries 0" <% if (eslintVersion) { %>,
		"lint": "eslint" <% } %>
    },
	"devDependencies": {
		<% if (eslintVersion) { %> "eslint": "<%= eslintVersion %>" <% } %>
	},
    "dependencies": {
        "mbt": "<%= mbtVersion %>"
    },
    "workspaces": []
}
